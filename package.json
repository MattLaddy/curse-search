{
    "name": "curse-search",
    "displayName": "Curse Search",
    "version": "0.0.1",
    "preview": true,
    "description": "A VSCode extension for recursive function call searching",
    "publisher": "MattLaddy",
    "icon": "images/logo.png",
    "license": "SEE LICENSE IN LICENSE",
    "main": "dist/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run build",
        "compile": "cross-env NODE_ENV=development ts-node --files -P ./scripts/tsconfig.json ./scripts",
        "watch": "cross-env NODE_ENV=development ts-node --files -P ./scripts/tsconfig.json ./scripts --watch",
        "test-compile": "tsc -p ./test/tsconfig.json",
        "watch-tests": "tsc -w -p ./test/tsconfig.json",
        "build": "cross-env NODE_ENV=production ts-node --files -P ./scripts/tsconfig.json ./scripts",
        "build-analyze": "npm run build -- --analyze",
        "lint": "eslint -c .eslintrc.js --ext .ts ./{scripts,src,test}/**/*.ts",
        "test": "npm run compile && npm run test-compile && node ./dist/test/runTests.js",
        "prepare": "husky install"
    },
    "author": {
        "name": "MattLaddy"
    },
    "badges": [
        {
            "url": "https://img.shields.io/badge/License-MIT-brightgreen.svg",
            "description": "License: MIT",
            "href": "https://github.com/mattladdy/curse-search/blob/master/LICENSE"
        },
        {
            "url": "https://img.shields.io/badge/PRs-welcome-brightgreen.svg",
            "description": "PRs Welcome",
            "href": "https://github.com/mattladdy/curse-search"
        }
    ],
    "homepage": "https://github.com/mattladdy/curse-search/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/mattladdy/curse-search/issues",
        "email": "mattladdy@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mattladdy/curse-search"
    },
    "engines": {
        "vscode": "^1.73.0",
        "node": "^16.8.1"
    },
    "categories": [
        "Programming Languages",
        "Other"
    ],
    "keywords": [
        "vscode",
        "search",
        "recursive",
        "function",
        "call graph"
    ],
    "activationEvents": [
        "onCommand:CurseSearch.recursiveSearch"
    ],
    "contributes": {
        "commands": [
            {
                "command": "CurseSearch.recursiveSearch",
                "title": "Recursive Function Search"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection",
                    "command": "CurseSearch.recursiveSearch",
                    "group": "navigation"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "curse-search",
                    "title": "Curse Search",
                    "icon": "images/logo.png"
                }
            ]
        },
        "views": {
            "curse-search": [
                {
                    "id": "curseSearch.resultsView",
                    "name": "Search Results",
                    "type": "webview"
                }
            ]
        }
    },
    "lint-staged": {
        "*.ts": [
            "eslint -c .eslintrc.js",
            "format-imports --config .vscode/import-sorter.json"
        ],
        "*.{ts,json,md}": [
            "prettier --write"
        ]
    },
    "dependencies": {
        "@babel/parser": "^7.22.5",
        "@babel/traverse": "^7.22.5",
        "@babel/types": "^7.22.5"
    },
    "devDependencies": {
        "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
        "@types/babel__traverse": "^7.20.1",
        "@types/glob": "^8.0.0",
        "@types/mocha": "^10.0.0",
        "@types/node": "^16.18.3",
        "@types/terser-webpack-plugin": "^5.2.0",
        "@types/vscode": "^1.73.0",
        "@types/webpack": "^5.28.0",
        "@types/webpack-bundle-analyzer": "^4.6.0",
        "@types/webpack-merge": "^5.0.0",
        "@types/webpackbar": "^4.0.3",
        "@types/yargs": "^17.0.13",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "@vscode/test-electron": "^2.2.0",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-unicorn": "^44.0.2",
        "format-imports": "^3.1.2",
        "glob": "^8.0.3",
        "husky": "^8.0.2",
        "lint-staged": "^13.0.3",
        "mocha": "^10.1.0",
        "prettier": "^2.7.1",
        "speed-measure-webpack-plugin": "^1.5.0",
        "terser-webpack-plugin": "^5.3.6",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "webpack": "^5.74.0",
        "webpack-bundle-analyzer": "^4.7.0",
        "webpack-merge": "^5.8.0",
        "webpackbar": "^5.0.2",
        "yargs": "^17.6.2"
    }
}
